{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "gaggle",
      "image": "jordanyono/gaggle",
      "hostname": "gaggle",
      "essential": false,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 3050
        }
      ]
    },
    {
      "name": "server",
      "image": "jordanyono/gaggle-api",
      "hostname": "api",
      "essential": false,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 4000,
          "containerPort": 4000
        }
      ]
    },
    {
      "name": "nginx",
      "image": "jordanyono/gaggle-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["gaggle", "server"],
      "memory": 128
    }
  ]
}
