{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "development"
        },
        {
          "name": "PORT",
          "value": "4000"
        }
      ],
      "essential": false,
      "hostname": "api",
      "image": "jordanyono/gaggle-api",
      "memory": 128,
      "name": "api"
    },
    {
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "development"
        },
        {
          "name": "PORT",
          "value": "3000"
        }
      ],
      "essential": false,
      "name": "gaggle",
      "hostname": "gaggle",
      "image": "jordanyono/gaggle",
      "memory": 128
    },
    {
      "essential": true,
      "name": "nginx",
      "image": "jordanyono/gaggle-nginx",
      "memory": 128,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "links": ["gaggle", "api"]
    }
  ]
}
