{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "development"
        },
        {
          "name": "PORT",
          "value": "4000"
        }
      ],
      "essential": false,
      "image": "jordanyono/gaggle-api",
      "memory": 128,
      "name": "api",
      "portMappings": [
        {
          "containerPort": 4000,
          "hostPort": 4000
        }
      ]
    },
    {
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "development"
        },
        {
          "name": "PORT",
          "value": "4900"
        }
      ],
      "essential": false,
      "name": "gaggle",
      "image": "jordanyono/gaggle",
      "memory": 128,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 4900
        }
      ]
    },
    {
      "essential": true,
      "name": "nginx",
      "image": "jordanyono/gaggle-nginx",
      "memory": 128,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ],
      "links": ["gaggle", "api"]
    }
  ]
}
