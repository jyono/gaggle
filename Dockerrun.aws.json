{
  "AWSEBDockerrunVersion": "2",
  "containerDefinitions": [
    {
      "name": "gaggle",
      "image": "jordanyono/gaggle",
      "hostname": "gaggle",
      "essential": false,
      "memory": 128
    },
    {
      "name": "api",
      "image": "jordanyono/gaggle-api",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginx",
      "image": "jordanyono/gaggle-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["gaggle", "api"],
      "memory": 128
    }
  ]
}
